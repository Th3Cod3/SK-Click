{% extends 'layout.twig' %}
{% block content %}
	<div class="mt-5" id="division-dashboard">
		<div class="container">
			<div class="card">
				<div class="card-header text-center font-weight-bold">
					#{ division.name }
				</div>
				<div class="card-body main-card">
					<div class="loader"></div>
						<div class="row">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}

{% block javascript %}
	<script>
		let login = new Vue({
			el: "#app-body",
			delimiters: ["#{", "}"],
			data(){
				return {
					division: {{ division | json_encode | raw }}
				}
			}
		});
	</script>	
{% endblock %}
